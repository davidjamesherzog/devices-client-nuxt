{
  "name": "devices-client-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "dev:server": "nest start --debug --watch",
    "build": "nest build && nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*.(js|ts|vue)\"",
    "test:client": "jest",
    "test:server": "jest --config ./jest-server.config.js",
    "test:e2e": "jest --config ./server/test/jest-e2e.json",
    "test": "npm run test:client && npm run test:server",
    "test-all": "npm run test:client && npm run test:server && npm run test:e2e"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/typeorm": "^7.1.5",
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "pg": "^8.5.1",
    "core-js": "^3.8.3",
    "node-sass": "^5.0.0",
    "nuxt": "^2.14.12",
    "nuxt-buefy": "^0.4.4",
    "nuxt-property-decorator": "^2.9.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "sass-loader": "^10.1.1",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1",
    "vuex-class": "^0.3.2",
    "vuex-class-component": "^2.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/jest": "^26.0.22",
    "@types/supertest": "^2.0.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "vue-jest": "^3.0.4"
  }
}
